CREATE FUNCTION guardardiferencia(dif double precision, codigo character varying, texto character varying) RETURNS boolean
    LANGUAGE plpgsql
    AS $$

DECLARE

codpersona	CHARACTER VARYING;
cuotaMax	INTEGER;
cuota		INTEGER;
resultado 	BOOLEAN;
obs            CHARACTER VARYING;

BEGIN
	resultado = false;
	obs = '';
	
	SELECT codper 
	INTO codpersona
	FROM sbp_det_pincentivo_vivienda WHERE codincviv = codigo;
	
	IF (codpersona!='' AND dif>0) THEN

		SELECT MAX(cuacan)
		INTO cuotaMax 
		FROM sbp_det_pincentivo_vivienda WHERE codincviv = codigo;

		IF (cuotaMax::INTEGER<6) THEN
			cuota = 6;	
		ELSE
			cuota = cuotaMax+1;
		END IF;

		--RAISE NOTICE 'cuota: %', cuota;
		--RAISE NOTICE 'cuotaMax: %', cuotaMax;
		IF(texto<>'MONTO INICIAL') THEN
			obs = 'Diferencia de pago por ajuste salarial cuota nro:'||texto;
		ELSE
			obs = 'Diferencia de pago por ajuste salarial sobre monto inicial';
		END IF;
	
		INSERT INTO sbp_det_pincentivo_vivienda(
		    codincviv, codper, cuacan, pagemi, observacion)
		VALUES (codigo, codpersona, cuota, dif, obs);

		resultado = true;
	END IF;

	RETURN resultado;
END;
$$;


