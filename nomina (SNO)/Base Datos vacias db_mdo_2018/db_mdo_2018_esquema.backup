	CLOSE cu02;

	-- Si existe algula persona se termina de construir el correo para ser enviado
	IF mtextocorreo <> '' THEN
		mtextocorreo = 'Se le ha asignado Correo Electrónico Corporativo a los siguientes trabajadores de la empresa que han pasado a ser Personal Fijo de Tiempo Completo:<br>' ||
		'&nbsp;<br>' ||
		mtextocorreo ||
		'Atentamente,<br>' ||
		'&nbsp;<br>' ||
		'Departamento de Sistemas de Información.<br>' ||
		'MADERAS DEL ORINOCO C.A.<br>' ||
		'&nbsp;<br>' ||
		'***************AVISO IMPORTANTE***************<br>' ||
		'Este mensaje fue generado de forma automatizada; contiene información de interés sólo para MADERAS DEL ORINOCO. Su copia, distribución o uso sólo está permitida a personas autorizadas. Si recibió este correo por error, destrúyalo. <br>';


	    -- Se realiza el intento de envio de correo
	    BEGIN
		--SELECT INTO respuesta pypgmail('correo.maderasdelorinoco.gob.ve',587,'webmaster@maderasdelorinoco.gob.ve','webmaster@maderasdelorinoco.gob.ve','0x1g3no3128','Asignación de correos electrónicos corporativos',mtextocorreo,'');
		SELECT INTO respuesta pypgmail('smtp.gmail.com',587,'sissermdo@gmail.com','sissermdo@gmail.com','Af2601ad','Asignación de correos electrónicos corporativos',mtextocorreo,'');
		--Se envia este correo en respaldo mientras se estabiliza el script
		SELECT INTO respuesta pypgmail('correo.maderasdelorinoco.gob.ve',587,'antonio.salazar@maderasdelorinoco.gob.ve','antonio.salazar@maderasdelorinoco.gob.ve','#abc12380#','Asignación de correos electrónicos corporativos',mtextocorreo,'');
		
	        EXCEPTION
	        -- Si ocurre algun error con el envio de correo se retorna 0
		    WHEN internal_error THEN
		         RETURN 0; -- Cuando ha ocurrido algun error
	    END;

	    -- Si no ha tenido problema al enviar el correo
	    TRUNCATE TABLE tmp_correo_corporativo;

	END IF;

	RETURN 1; -- Cuando ha sido satisfactorio
 END;
$$;


