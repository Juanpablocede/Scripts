CREATE FUNCTION obtiene(nombre character varying, apellido character varying) RETURNS character varying
    LANGUAGE plpgsql
    AS $$ 
--Funcion que obtiene el primer nombre y el primer apellido
DECLARE

 nomape  CHARACTER VARYING(60);
 ape	 CHARACTER VARYING(60);
 apell	 CHARACTER VARYING;
 cadena	 CHARACTER VARYING;
 i	 INTEGER;
 t	 INTEGER;
 bandera BOOLEAN;

BEGIN

  ape = (SELECT (regexp_split_to_table(apellido,E'\\s+')) limit 1);
  ape = (SELECT TRIM(ape));
  apell = '';
  bandera = true;
  
  IF (ape = 'DEL' OR ape = 'DE') THEN
  
	t = LENGTH(ape);
	i = 2;

	cadena = SUBSTR(apellido, t+i , 1); 

	WHILE (bandera<>false) LOOP
		cadena = SUBSTR(apellido, t+i , 1); 
		IF(cadena<>'' AND cadena<>' ') THEN
			apell = apell||cadena;
			i = i + 1;
		ELSE
		   bandera = false;
		END IF;
	END LOOP;
	
	nomape = ((SELECT (regexp_split_to_table(nombre,E'\\s+')) LIMIT 1)||' '||ape||' '||apell);
  ELSE
	nomape = ((select (regexp_split_to_table(nombre,E'\\s+')) limit 1)||' '||(select (regexp_split_to_table(apellido,E'\\s+')) limit 1));
  END IF;
  
RETURN nomape;
END;

$$;


