			-- Si no existen ya en la tabla temporal la persona
			IF mcuenta1=0 THEN
				--Se ingresa la persona en una tabla temporal por si ocurre alguna falla
				INSERT INTO tmp_correo_corporativo(codper, cedper, nomper, apeper, dendep, denger, coreleper, fecingper)
				VALUES (xcodper, xcedper, xnomper, xapeper, xdendep, xdenger, mcoreleper, xfecingper);
