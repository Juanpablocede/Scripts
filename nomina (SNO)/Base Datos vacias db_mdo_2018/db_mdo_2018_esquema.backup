CREATE FUNCTION crearcorreopersonal() RETURNS integer
    LANGUAGE plpgsql
    AS $$

DECLARE

	respuesta    INTEGER;
	msw01        INTEGER;
	msw02        INTEGER;
	mcuenta1     INTEGER;
	mcuenta2     INTEGER;
	mcontador    INTEGER;
	mnombres     CHARACTER(60);
	mapellidos   CHARACTER(60);
	mletranom    CHARACTER(1);
	mletraape    CHARACTER(1);
	mcoreleper   CHARACTER VARYING(100); -- Correo electrónico nuevo del personal
	mtextocorreo TEXT; -- Correo para enviar al Departamento de Soporte Técnico
	copia TEXT; -- Correo para enviar al Departamento de Soporte Técnico
	
	xcodper      CHARACTER VARYING(10);  -- Cedula personal
	xcedper      CHARACTER VARYING(10);  -- Cedula personal
	xnomper      CHARACTER VARYING(60);  -- Nombres personal
	xapeper      CHARACTER VARYING(60);  -- Apellidos personal
	xdendep      CHARACTER VARYING(254); -- Descripción departamento
	xdenger      CHARACTER VARYING(254);  -- Descripción gerencia
	xfecingper   DATE;                   -- Fecha ingreso personal

	cu01 CURSOR FOR

		--Consulta para incorporar los correos creados con anterioridad y que por fallos no pudieron reportarse
		SELECT codper,
		       cedper,
		       nomper,
		       apeper,
		       dendep,
		       denger,
		       coreleper,
		       fecingper
		FROM tmp_correo_corporativo
		ORDER BY nomper;
		
	cu02 CURSOR FOR

		--Consulta para determinar todos los empleados que no poseen correo electrónico y ya han pasado a fijos
		SELECT t1.codper,
		       cedper,
		       nomper,
		       apeper,
		       dendep,
		       denger,
		       t1.fecingper
		FROM sno_personal t1
			INNER JOIN sno_personalnomina t2 ON (t1.codper = t2.codper)
			LEFT JOIN srh_departamento t3 ON ((t2.coddep = t3.coddep) OR (t3.coddep = '00000'|| t1.codorg))
			LEFT JOIN srh_gerencia t4 ON (t1.codger = t4.codger)
		WHERE (coreleper IS NULL OR TRIM(coreleper) = '' OR coreleper NOT LIKE '%@maderasdelorinoco.gob.ve')
		    AND codded = '100' --AND codded = '300' Para pruebas se coloca 300 que es contratado
		AND ((t2.codnom = '0000'
		     OR t2.codnom = '0001'
		     OR t2.codnom = '0002'
		     OR t2.codnom = '0003'
		     OR t2.codnom = '0004'
		     OR t2.codnom = '0009'
		     OR t2.codnom = '0006'
		     OR t2.codnom = '0007'
		     OR t2.codnom = '0008')
			OR
		     (t2.codnom = '9000'
		     OR t2.codnom = '9001'
		     OR t2.codnom = '9002'
		     OR t2.codnom = '9003'
		     OR t2.codnom = '9004'
		     OR t2.codnom = '9005')
		     )
		     AND (t2.staper = '1' OR t2.staper = '2' )
		     AND t1.estper = '1'
		GROUP BY t1.codper,
			 cedper,
		         nomper,
		         apeper,
		         dendep,
		         denger,
		         t1.fecingper
		ORDER BY nomper; -- LIMIT 3; Para pruebas se limitan los registros


BEGIN
	mtextocorreo = '';

	/*********************************************************************************************/
	/** Se anexan al correo que se va a enviar las personas que con anterioridad se les         **/
	/** ha asignado correo corporativo pero por algun fallo no pudo notificarse antes mediante  **/
	/** correo electrónico - P.ej. falla de internet, falla de servicio de correo, etc.         **/
	/*********************************************************************************************/
	
	OPEN cu01;
	 -- Genero Etiqueta para Hacer Ciclico 
	<<buscar_datos>>
	LOOP
	BEGIN
		FETCH cu01 INTO 
		        xcodper,
			xcedper,
			xnomper,
			xapeper,
			xdendep,
			xdenger,
			mcoreleper,
			xfecingper;

	        IF NOT FOUND THEN
	           EXIT buscar_datos;
	        END IF;			
			-- Se va construyendo el correo para enviarlo al Departamento de Soporte Técnico
			mtextocorreo = mtextocorreo || 
			'<b>Cédula:</b> ' || xcedper || '<br>' ||
			'<b>Nombres:</b> ' || xnomper || '<br>' ||
			'<b>Apellidos:</b> ' || xapeper || '<br>' ||
			'<b>Correo:</b> ' || mcoreleper || '<br>';
			
			--Si la persona tiene departamento asignado
			IF xdendep <> '' THEN
				mtextocorreo = mtextocorreo || '<b>Departamento:</b> ' || xdendep || '<br>';
			ELSE
				mtextocorreo = mtextocorreo || '<b>Departamento:</b> NO ASIGNADO<br>';
			END IF;

			--Si la persona tiene generencia asignada
			IF xdenger <> '' THEN
				mtextocorreo = mtextocorreo || '<b>Gerencia:</b> ' || xdenger || '<br>';
			ELSE
				mtextocorreo = mtextocorreo || 'Gerencia:</b> NO ASIGNADO<br>';
			END IF;
			
			mtextocorreo = mtextocorreo || '&nbsp;<br>';
		END;

	END LOOP;
	CLOSE cu01;

	/*********************************************************************************************/
	/** Se crean los correos para las personas que no lo tienen y se anexan al correo que se va **/
	/** a enviar al Departamento de Soporte Tecnico                                             **/
	/*********************************************************************************************/

	OPEN cu02;
	 -- Genero Etiqueta para Hacer Ciclico 
	<<buscar_datos>>
	LOOP
	BEGIN
		FETCH cu02 INTO 
		        xcodper,
			xcedper,
			xnomper,
			xapeper,
			xdendep,
			xdenger,
			xfecingper;

	        IF NOT FOUND THEN
	           EXIT buscar_datos;
	        END IF;
			-- Inicio del proceso de creación del email del personal
			mnombres = '';
			mapellidos = '';

			msw01 = 0;
			msw02 = 0;
		  
			mcontador:=1;
			WHILE mcontador<=60 LOOP
		        IF msw01=0 THEN
		            mletranom:=LOWER(SUBSTRING(xnomper,mcontador,1));
		            IF mletranom='Ñ' THEN
		                mletranom='n';
		            END IF;
		            IF mletranom='Á' THEN
		                mletranom='a';
		            END IF;
		            IF mletranom='É' THEN
		                mletranom='e';
		            END IF;
		            IF mletranom='Í' THEN
		                mletranom='i';
		            END IF;
		            IF mletranom='Ó' THEN
		                mletranom='o';
		            END IF;
		            IF mletranom='Ú' THEN
		                mletranom='u';
		            END IF;
		            IF (mletranom=' ') THEN 
		                msw01=1;
		            ELSE
		                mnombres:=mnombres||(mletranom);
		            END IF;
		        END IF;
		        
		        IF msw02=0 THEN
		            mletraape:=LOWER(SUBSTRING(xapeper,mcontador,1));
		            IF mletraape='Ñ' THEN
		                mletraape='n';
		            END IF;
		            IF mletraape='Á' THEN
		                mletraape='a';
		            END IF;
		            IF mletraape='É' THEN
		                mletraape='e';
		            END IF;
		            IF mletraape='Í' THEN
		                mletraape='i';
		            END IF;
		            IF mletraape='Ó' THEN
		                mletraape='o';
		            END IF;
		            IF mletraape='Ú' THEN
		                mletraape='u';
		            END IF;
		            IF (mletraape=' ') AND mapellidos<>'de' THEN 
		                msw02=1;
		            ELSE
		                mapellidos:=mapellidos||(mletraape);
		            END IF;
		        END IF;
		        mcontador:=mcontador+1;
			END LOOP;

			mcuenta2:=0;
		        mcuenta2:=COUNT(*) FROM sno_personal WHERE sno_personal.coreleper = LOWER(mnombres)||'.'||LOWER(mapellidos)||'@maderasdelorinoco.gob.ve';

			-- Si no existe una persona con un correo igual al nombre.apellido@maderasdelorinoco.gob.ve
			IF mcuenta2=0 THEN
				mcoreleper = mnombres||'.'||mapellidos||'@maderasdelorinoco.gob.ve';
				UPDATE sno_personal SET coreleper=mcoreleper WHERE sno_personal.codper=xcodper;
			END IF;

			-- Si existe una persona con un correo igual al nombre.apellido@maderasdelorinoco.gob.ve se agregan los 3 ultimos digitos de la cedula
			IF mcuenta2>0 THEN
				mcoreleper = mnombres||'.'||mapellidos||SUBSTRING(xcedper,length(xcedper)-2,length(xcedper))||'@maderasdelorinoco.gob.ve';
				UPDATE sno_personal SET coreleper=mcoreleper WHERE sno_personal.codper=xcodper;
