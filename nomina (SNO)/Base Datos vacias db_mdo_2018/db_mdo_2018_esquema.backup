CREATE FUNCTION actualiza_expediente(OUT codper character, OUT fecingper date) RETURNS SETOF record
    LANGUAGE plpgsql
    AS $_$

DECLARE
  mcodper 	ALIAS FOR $1;
  mfecingper	ALIAS FOR $2;
  mcorrelativo	INTEGER; 
  existen_datos BOOLEAN;
  
cu01 CURSOR FOR 

SELECT 
	sno_personal.codper,
	sno_personal.fecingper
FROM 
	sno_personal
ORDER BY
	sno_personal.fecingper;
BEGIN
 existen_datos = FALSE;

 mcorrelativo:=0;
 
OPEN cu01;
<<buscar_datos>>
 LOOP
   BEGIN
     FETCH cu01 INTO
	  mcodper,
	  mfecingper;
	  IF NOT FOUND THEN
             EXIT buscar_datos;
          END IF;

	  mcorrelativo:=mcorrelativo + 1;
	  
	  RAISE NOTICE 'valor  %',mcorrelativo;

	  UPDATE sno_personal SET numexpper = mcorrelativo WHERE sno_personal.codper=mcodper;

     RETURN NEXT;
   END;
 END LOOP;
 CLOSE cu01;
END;
$_$;


