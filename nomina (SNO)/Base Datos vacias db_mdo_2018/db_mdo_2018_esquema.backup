CREATE FUNCTION obtenerproxcorrelativo(tablename character varying, fieldname character varying, incvalue numeric) RETURNS numeric
    LANGUAGE plpgsql
    AS $$
DECLARE
 rec RECORD;
 var_max NUMERIC;
BEGIN
  FOR rec IN EXECUTE 'Select CAST(max('|| FieldName ||') AS NUMERIC) as maxid from ' || TableName LOOP
   var_max := rec.maxid::NUMERIC;
  END LOOP;
 
  IF var_max IS NULL THEN
   RETURN 1;
  ELSE
   RETURN var_max + IncValue;
  END IF;
  RETURN 1;
END;
$$;


