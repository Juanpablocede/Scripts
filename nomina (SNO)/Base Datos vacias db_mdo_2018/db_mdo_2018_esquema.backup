--
-- TOC entry 980 (class 1255 OID 535973)
-- Name: calcularmonto(character varying, character varying, character varying, double precision); Type: FUNCTION; Schema: public; Owner: postgres
--

CREATE FUNCTION calcularmonto(cedula character varying, cuota character varying, nomina character varying, sueldo double precision) RETURNS character varying
    LANGUAGE plpgsql
    AS $$

DECLARE

total	 		DOUBLE PRECISION;
sueldobasico 		DOUBLE PRECISION;
cantdias 		INTEGER;
cuo1	 		INTEGER;
cuo2	 		INTEGER;
cuo3	 		INTEGER;
cuo4	 		INTEGER;
cuo5	 		INTEGER;
registros 		INTEGER;


BEGIN

	SELECT COUNT(*) INTO registros FROM sbp_det_pincentivo_vivienda WHERE codper LIKE '%'||cedula||'%' AND cuacan::INTEGER<6;

	SELECT candia, cuota1, cuota2, cuota3, cuota4, cuota5 
	INTO cantdias, cuo1, cuo2, cuo3, cuo4, cuo5
	FROM sbp_diaspago_piv 
	WHERE codnom = nomina;

	IF(nomina::INTEGER>=0000 AND nomina::INTEGER<=0005) THEN

		sueldobasico = sueldo/30;
	ELSE
		sueldobasico = sueldo;
	END IF;
		

	IF (registros=1) THEN
	
		total = sueldobasico * cantdias;	
	ELSE
		IF(cuota='1') THEN
			total = sueldobasico* cuo1;
		END IF;

		IF(cuota='2') THEN
			total = sueldobasico* cuo2;
		END IF;

		IF(cuota='3') THEN
			total = sueldobasico* cuo3;
		END IF;

		IF(cuota='4') THEN
			total = sueldobasico* cuo4;
		END IF;

		IF(cuota='5') THEN
			total = sueldobasico* cuo5;
		END IF;
	
	END IF;

	RETURN total; 	
END;
$$;


