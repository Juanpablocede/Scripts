-- TOC entry 1030 (class 1255 OID 536019)
-- Name: guardar_recalculoincentivovivienda(character varying, double precision, character varying); Type: FUNCTION; Schema: public; Owner: postgres
--

CREATE FUNCTION guardar_recalculoincentivovivienda(codigo character varying, sueldo double precision, cedula character varying) RETURNS character varying
    LANGUAGE plpgsql
    AS $$

-- Nombre:	recalcular_incentivovivienda
-- Objetivo:	Devuelve una cadena que contiene el recalculo de las cuotas de plan incentivo de vivienda luego de ajuste salarial

DECLARE

codpersona             CHARACTER(10);		-- Identificador del personal
codnomina	       CHARACTER (4);
sueldosolicitud        DOUBLE PRECISION;	-- Sueldo actual
cantidadias	       INTEGER;			-- cantidad de dias a cancelar
sueldobasico	       DOUBLE PRECISION;	-- sueldo basico del trabajador	
monto		       DOUBLE PRECISION;
i	               INTEGER;
cantidad	       INTEGER;
cant		       INTEGER;
cuo1			INTEGER;
cuo2			INTEGER;
cuo3			INTEGER;
cuo4			INTEGER;
cuo5			INTEGER;
resultado              CHARACTER VARYING;                -- Determina si hubo o no cambios en la ejecución del script

obtSueldo CURSOR FOR			       -- Sueldo actual del personal nomina mensual

SELECT
sno_personalnomina.codper,
sno_personalnomina.codnom,
sno_personalnomina.fecingper,
antper 

FROM sno_personal

INNER JOIN sno_personalnomina ON sno_personalnomina.codper = sno_personal.codper
INNER JOIN sbp_pincentivo_vivienda ON (sbp_pincentivo_vivienda.codper = sno_personalnomina.codper AND sbp_pincentivo_vivienda.codnom = sno_personalnomina.codnom)

WHERE sno_personalnomina.codnom BETWEEN '0000' AND '0010' AND codincviv=codigo AND sno_personal.cedper= cedula AND estben='A' OR estben='AC';

BEGIN
	---------------------------------------
	--CURSOR DE LOS SUELDOS NOMINA MENSUAL--
	---------------------------------------
	i=1;
	monto = 0;
	
	--Se recorre el cursor de los Sueldos
	OPEN obtSueldo;
	 -- Genero Etiqueta para Hacer Ciclico 
	<<buscar_datos>>
	LOOP
		BEGIN
			FETCH obtSueldo INTO  
			codpersona,    
			codnomina;

			IF NOT FOUND THEN
			   EXIT buscar_datos;
			END IF;

			--se obtiene el sueldo basico al momento de la solicitud
			SELECT sueper
			INTO sueldosolicitud
		        FROM sno_hpersonalnomina
		        INNER JOIN sbp_pincentivo_vivienda ON sno_hpersonalnomina.codper = sbp_pincentivo_vivienda.codper
				 AND sno_hpersonalnomina.codperi = sbp_pincentivo_vivienda.codperi
				 AND sno_hpersonalnomina.codnom = sbp_pincentivo_vivienda.codnom
				 AND sno_hpersonalnomina.anocur = sbp_pincentivo_vivienda.anocur;

			--Obtengo la cantidad de dias de acuerdo al tipo de nomina
			SELECT candia, cuota1, cuota2, cuota3, cuota4, cuota5
			INTO cantidadias, cuo1, cuo2, cuo3, cuo4, cuo5
			FROM sbp_diaspago_piv
			WHERE codnom = codnomina;
	
			--Si el nuevo sueldo es diferente que el sueldo anterior
			IF sueldo <> sueldosolicitud THEN

				--se calcula el sueldo basico del empleado
				IF ((codnomina::INTEGER)>=0000 AND (codnomina::INTEGER)<=0005) THEN
					sueldobasico = sueldo/30;
				ELSE
					sueldobasico= sueldo;
				END IF;
				
				SELECT COUNT(cuacan) INTO cantidad FROM sbp_det_pincentivo_vivienda WHERE cuacan::INTEGER<6 AND codincviv=codigo AND codper=codpersona;

				IF(cantidad>1) THEN
					WHILE (i<=5) LOOP
						SELECT COUNT(cuacan) INTO cant FROM sbp_det_pincentivo_vivienda WHERE cuacan::INTEGER<6 AND codincviv=codigo AND codper=codpersona AND estsol!='E' AND estsol!='C' AND estsol!='P';
						IF (cant>0) THEN
							IF (i=1) THEN
								monto = sueldobasico*cuo1;
							ELSE
								IF(i=2) THEN
									monto = sueldobasico*cuo2;
								END IF;

								IF(i=3) THEN
									monto = sueldobasico*cuo3;
								END IF;

								IF(i=4) THEN
									monto = sueldobasico*cuo4;
								END IF;

								IF(i=5) THEN
									monto = sueldobasico*cuo5;
								END IF;
								
							END IF;

							UPDATE sbp_det_pincentivo_vivienda
							SET  pagemi=monto
							WHERE codincviv = codigo AND  codper = codpersona AND cuacan= i::CHARACTER AND estsol!='E' AND estsol!='C' AND estsol!='P'
							      AND codincviv NOT IN (SELECT codsol FROM sbp_det_solicitudes WHERE numpag LIKE i::CHARACTER AND codsol = codigo);

							resultado = 'TRUE';
						ELSE
							resultado = 'FALSE';

						END IF;
						i = i+1;
					END LOOP;
				ELSE
					SELECT COUNT(cuacan) INTO cant FROM sbp_det_pincentivo_vivienda WHERE cuacan::INTEGER<6 AND codincviv=codigo AND codper=codpersona AND estsol!='E' AND estsol!='C' AND estsol!='P';

					IF (cant=1) THEN
												
						monto = sueldobasico*cantidadias;

						UPDATE sbp_det_pincentivo_vivienda
							SET  pagemi=monto
							WHERE codincviv = codigo AND  codper = codpersona AND cuacan= '1' AND estsol!='E' AND estsol!='C' AND estsol!='P'
							AND codincviv NOT IN (SELECT codsol FROM sbp_det_solicitudes WHERE numpag LIKE i::CHARACTER AND codsol = codigo);
						resultado = 'TRUE';
					ELSE
						resultado = 'FALSE';

					END IF;

				END IF;
			ELSE
				resultado = 'FALSE';
			
			END IF;
		END;
	END LOOP;
	CLOSE obtSueldo;

	RETURN resultado; 
END;
$$;


