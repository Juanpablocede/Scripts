CREATE FUNCTION proxcumpleanios(fecingreso text) RETURNS text
    LANGUAGE plpgsql STABLE
    AS $$
		DECLARE

			mesing	INTEGER;
			diaing	INTEGER;
			annoing	INTEGER;

			mes	INTEGER;
			dia	INTEGER;
			anno	INTEGER;

			proximocumple TEXT;
  
		BEGIN

			SELECT EXTRACT(YEAR FROM fecingreso::timestamp) INTO annoing;
			SELECT EXTRACT(MONTH FROM fecingreso::timestamp) INTO mesing;
			SELECT EXTRACT(DAY FROM fecingreso::timestamp) INTO diaing;

			SELECT EXTRACT(MONTH FROM CURRENT_DATE) INTO mes;
			SELECT EXTRACT(DAY FROM CURRENT_DATE) INTO dia;

			SELECT EXTRACT(YEAR FROM CURRENT_DATE) INTO anno;

			RAISE NOTICE 'dia: %', dia; RAISE NOTICE 'mes: %', mes;	 RAISE NOTICE 'anno: %', anno;	
			RAISE NOTICE 'diaing: %', diaing; RAISE NOTICE 'mesing: %', mesing;	 RAISE NOTICE 'annoing: %', annoing;		

			IF (mesing <= mes AND diaing <= dia) THEN
				proximoCumple = anno+1||'/'||mesing||'/'||diaing;
			ELSE
				proximoCumple = anno||'/'||mesing||'/'||diaing;
			END IF;

			RETURN proximoCumple;
		END;
	$$;


