-- TOC entry 1027 (class 1255 OID 536016)
-- Name: definicion_salarial(); Type: FUNCTION; Schema: public; Owner: postgres
--

CREATE FUNCTION definicion_salarial(OUT codper character, OUT codnom character, OUT codperi character, OUT monto double precision) RETURNS SETOF record
    LANGUAGE plpgsql
    AS $_$

DECLARE
  mcodper 	ALIAS FOR $1;
  mcodnom 	ALIAS FOR $2;
  mcodperi	ALIAS FOR $3;
  mmonto	ALIAS FOR $4;
  existen_datos BOOLEAN;
  
cu01 CURSOR FOR 

SELECT
	sno_hsalida.codper,
	sno_hsalida.codnom,
	sno_hsalida.codperi,
	sum(sno_hsalida.valsal) as monto
FROM
	sno_hsalida,
	sno_concepto
WHERE
	sno_hsalida.codconc=sno_concepto.codconc AND
	sno_hsalida.codnom=sno_concepto.codnom AND
	sno_hsalida.codnom BETWEEN '0000' AND '0010' AND 
	sno_concepto.sueintcon=1
GROUP BY 
	sno_hsalida.codper,
	sno_hsalida.codnom,
	sno_hsalida.codperi
ORDER BY
	sno_hsalida.codper;
BEGIN
 existen_datos = FALSE;

OPEN cu01;
<<buscar_datos>>
 LOOP
   BEGIN
     FETCH cu01 INTO
	  mcodper,
	  mcodnom,
	  mcodperi,
	  mmonto;
	  IF NOT FOUND THEN
             EXIT buscar_datos;
          END IF;

	  RAISE NOTICE 'valor  %',mcodper;


	  UPDATE sno_definicion_salarial SET sueldo_integral = mmonto WHERE mcodper=sno_definicion_salarial.codper AND mcodnom=sno_definicion_salarial.codnom AND mcodperi=sno_definicion_salarial.codperi;



     RETURN NEXT;
   END;
 END LOOP;
 CLOSE cu01;
END;
$_$;


ALTER FUNCTION public.definicion_salarial(OUT codper character, OUT codnom character, OUT codperi character, OUT monto double precision) OWNER TO postgres;

--
-- TOC entry 1028 (class 1255 OID 536017)
-- Name: depreciacion(); Type: FUNCTION; Schema: public; Owner: postgres
--

CREATE FUNCTION depreciacion(OUT codemp character, OUT codact character, OUT ideact character, OUT feccmpact date, OUT costo double precision, OUT vidautil double precision) RETURNS SETOF record
    LANGUAGE plpgsql
    AS $_$

DECLARE
  mcodemp 	ALIAS FOR $1;
  mcodact 	ALIAS FOR $2;
  mideact 	ALIAS FOR $3;
  mfeccmpact	ALIAS FOR $4;
  mcosto	ALIAS FOR $5;
  mvidautil	ALIAS FOR $6;
  mmeses 	INTEGER;
  mcontador	INTEGER;
  mdepanual	DOUBLE PRECISION;
  mdepmensual	DOUBLE PRECISION;
  mfechadep	DATE;
  mdepacum	DOUBLE PRECISION;
  mmes		INTEGER; --para saber el mes de la fecha de compra
  mdias		INTEGER; --para determinar los meses de 28,30 y 31 dias
  manos		INTEGER; --para determinar el año de la fecha de compra. Estas tres varables son para concatenar le fecha de depreciacion
  mfdia		CHARACTER(2);
  mfmes		CHARACTER(2);
  existen_datos BOOLEAN;
  
cu01 CURSOR FOR 

SELECT
	saf_activo.codemp,
	saf_activo.codact,
	saf_dta.ideact,
	saf_activo.feccmpact,
	saf_activo.costo,
	saf_activo.vidautil,
	--substr(cast(saf_activo.feccmpact as char(10)),6,2) AS mes
	EXTRACT (MONTH FROM saf_activo.feccmpact)

FROM
	saf_activo,
	saf_dta
WHERE
	saf_activo.codemp=saf_dta.codemp AND
	saf_activo.codact=saf_dta.codact AND
	saf_activo.vidautil>0 AND
	saf_activo.sc_cuenta<>'' AND
	--saf_activo.codact BETWEEN '000000000000001' AND '000000000013517'
	saf_activo.codact BETWEEN '000000000005399' AND '000000000005399'
ORDER BY
	saf_activo.codact;

BEGIN
 existen_datos = FALSE;

OPEN cu01;
<<buscar_datos>>
 LOOP
   BEGIN
     FETCH cu01 INTO
	  mcodemp,
	  mcodact,
	  mideact,
	  mfeccmpact,
	  mcosto,
	  mvidautil,
	  mmes;
	  IF NOT FOUND THEN
             EXIT buscar_datos;
          END IF;
          mcontador:=1;
	  mmeses:=(mvidautil*12);
	  mfechadep:=mfeccmpact;
	  mdepanual:=(mcosto/mvidautil);
	  mdepmensual:=(mcosto/(mvidautil*12));
	  mdepacum:=mdepmensual;
	  manos:=EXTRACT (YEAR FROM mfechadep);
	  WHILE mcontador<=mmeses LOOP
		mcontador:=mcontador+1;
		--mfechadep:=(mfechadep+31);
		mmes:=(mmes+1);
		IF mmes>12 THEN
			mmes:=1;
			manos:=(manos+1);
		END IF;
		
		IF mmes IN(1,3,5,7,8,10,12) THEN
			mdias=31;
		END IF;
		IF mmes IN(4,6,9,11) THEN
			mdias=30;
		END IF;		
		IF mmes IN(2) THEN
			mdias=28;
		END IF;
		IF mmes<10 THEN
			mfmes:='0'||mmes;
		ELSE
			mfmes:=mmes;
		END IF;
		
		IF mdias<10 THEN
			mfdia:='0'||mdias;
		ELSE
			mfdia:=mdias;
		END IF;
		mfechadep:=manos||'-'||mfmes||'-'||mfdia;
	
		
		INSERT INTO saf_depreciacion VALUES (mcodemp,mcodact,mideact,mfechadep,mdepmensual,mdepanual,mdepacum,0,mfechadep,'1900-01-01');
		mdepacum:=(mdepacum+mdepmensual);
	  END LOOP;
     RETURN NEXT;
   END;
 END LOOP;
 CLOSE cu01;
END;
$_$;


ALTER FUNCTION public.depreciacion(OUT codemp character, OUT codact character, OUT ideact character, OUT feccmpact date, OUT costo double precision, OUT vidautil double precision) OWNER TO postgres;

