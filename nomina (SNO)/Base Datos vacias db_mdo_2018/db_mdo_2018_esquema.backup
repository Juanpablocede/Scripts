--

CREATE FUNCTION recalcular_incentivovivienda(codigo character varying, sueldo double precision, cedula character varying) RETURNS character varying
    LANGUAGE plpgsql
    AS $$

-- Nombre:	recalcular_incentivovivienda
-- Objetivo:	Devuelve una cadena que contiene el recalculo de las cuotas de plan incentivo de vivienda luego de ajuste salarial

DECLARE

codpersona             CHARACTER(10);		-- Identificador del personal
codnomina	       CHARACTER (4);
sueldobasico	       DOUBLE PRECISION;	-- sueldo basico del trabajador
monto		       DOUBLE PRECISION;
i	               INTEGER;
cantidad               INTEGER;
cantdias	       INTEGER;
cuo1		       INTEGER;
cuo2		       INTEGER;
cuo3		       INTEGER;
cuo4		       INTEGER;
cuo5		       INTEGER;
resultado              CHARACTER VARYING;                -- Determina si hubo o no cambios en la ejecución del script

obtSueldo CURSOR FOR			       -- Sueldo actual del personal nomina mensual

SELECT
sno_personalnomina.codper,
sno_personalnomina.codnom

FROM sno_personal

INNER JOIN sno_personalnomina ON sno_personalnomina.codper = sno_personal.codper
INNER JOIN sbp_pincentivo_vivienda ON (sbp_pincentivo_vivienda.codper = sno_personalnomina.codper AND sbp_pincentivo_vivienda.codnom = sno_personalnomina.codnom)

WHERE sno_personalnomina.codnom BETWEEN '0000' AND '0010' AND codincviv=codigo AND sno_personal.cedper= cedula AND estben='AC';
-----estben='A' ESTABA ASI HAY QUE VER SI ESTE ESTATUS ES EL CORRECTO
BEGIN
	---------------------------------------
	--CURSOR DE LOS SUELDOS NOMINA MENSUAL--
	---------------------------------------
	i=1;
	monto = 0;
	resultado = '';
	
	--Se recorre el cursor de los Sueldos
	OPEN obtSueldo;
	 -- Genero Etiqueta para Hacer Ciclico 
	<<buscar_datos>>
	LOOP
		BEGIN
			FETCH obtSueldo INTO  
			codpersona,    
			codnomina;

			IF NOT FOUND THEN
			   EXIT buscar_datos;
			END IF;

			--se calcula el sueldo basico del empleado
			IF ((codnomina::INTEGER)>=0000 AND (codnomina::INTEGER)<=0005) THEN
				sueldobasico = sueldo/30;
			ELSE
				sueldobasico= sueldo;
			END IF;

			--se busca la cantidad de dias de acuerdo al tipo de nomina
			SELECT candia, cuota1, cuota2, cuota3, cuota4, cuota5
			INTO cantdias, cuo1, cuo2, cuo3, cuo4, cuo5
			FROM sbp_diaspago_piv 
			WHERE sbp_diaspago_piv.codnom = codnomina;
			
			--RAISE NOTICE 'sueldobasico: %', sueldobasico;	
			SELECT COUNT(cuacan) INTO cantidad FROM sbp_det_pincentivo_vivienda WHERE codincviv=codigo AND codper= codpersona AND cuacan::INTEGER<6;

			IF (cantidad>1) THEN
			
				WHILE (i<=5) LOOP
					IF (i=1) THEN
						monto = sueldobasico*cuo1;
						--RAISE NOTICE 'monto: %', monto;		
					ELSE
						IF (i=2) THEN
							monto = sueldobasico*cuo2;
						END IF;

						IF (i=3) THEN
							monto = sueldobasico*cuo3;
						END IF;

						IF (i=4) THEN
							monto = sueldobasico*cuo4;
						END IF;

						IF (i=5) THEN
							monto = sueldobasico*cuo5;
						END IF;	
					END IF;

					IF(i=5) THEN
						resultado = resultado||monto;
						--RAISE NOTICE 'resultado: %', resultado;
					ELSE
						resultado = resultado||monto||',';
						--RAISE NOTICE 'resultado: %', resultado;		

					END IF;
					i= i+1;
				END LOOP;
			ELSE

				resultado = sueldobasico* cantdias;

			END IF;
		END;
	END LOOP;
	CLOSE obtSueldo;

	RETURN resultado; 
END;
$$;


