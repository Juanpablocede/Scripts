--
-- TOC entry 986 (class 1255 OID 535971)
-- Name: autoincremento_str(character varying, character varying, numeric); Type: FUNCTION; Schema: public; Owner: postgres
--

CREATE FUNCTION autoincremento_str(tablename character varying, fieldname character varying, incvalue numeric) RETURNS character varying
    LANGUAGE plpgsql
    AS $$
DECLARE
 rec RECORD;
 var_max NUMERIC;
 longitud integer;
 resultado character varying;
BEGIN

  
  FOR rec IN EXECUTE 'Select CAST(max('|| FieldName ||') AS NUMERIC) as maxid from ' || TableName  LOOP
   var_max := rec.maxid::NUMERIC;
  END LOOP;
 
  IF var_max IS NULL THEN
   FOR rec IN EXECUTE 'Select rellenar_cero_izq AS resul FROM rellenar_cero_izq(1,'||longitud||')' LOOP
   	resultado := rec.resul;
   END LOOP;
  ELSE
   FOR rec IN EXECUTE 'Select '|| FieldName ||' AS campo FROM ' || TableName ||' LIMIT 1' LOOP
   	longitud := LENGTH(rec.campo);
   END LOOP;
   FOR rec IN EXECUTE 'Select rellenar_cero_izq AS resul FROM rellenar_cero_izq(('||var_max + IncValue||'),'||longitud||')' LOOP
   	resultado := rec.resul;
   END LOOP;
  END IF;
  RETURN resultado;
END;
$$;


