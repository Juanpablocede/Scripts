		i = 1;
		j = 1;
		monto  =0;
		respuesta = FALSE;
		
		--Se recorre el cursor de los Sueldos
		OPEN cursor1;
		 -- Genero Etiqueta para Hacer Ciclico  
		<<buscar_datos>>
		LOOP
			BEGIN
				FETCH cursor1 INTO      
				fechaing;

				IF NOT FOUND THEN
				   EXIT buscar_datos;
				END IF;

				--Obtenemos la cantidad de dias correspondiente a la nomina
				SELECT candia, cuota1, cuota2, cuota3, cuota4, cuota5
				INTO cantdias, cuo1, cuo2, cuo3, cuo4, cuo5
				FROM sbp_diaspago_piv WHERE codnom = codnomina;

				--Calculamos la antiguedad del empleado en relacion a la fecha del comite
				SELECT calcularantiguedad(fechaing::text, feccomite) INTO antiguedad;

				--Obtenemos el sueldo correspondiente al año y periodo enviado por parametro
				SELECT sueper INTO sueldo FROM sno_hpersonalnomina WHERE codperi = periodo AND anocur = anno AND codnom = codnomina
											AND codper= codpersonal;			

				--Si el tipo de nomina es mensual obtenemos el sueldo diario
				IF (codnomina::INTEGER >= 0000 AND codnomina::INTEGER <= 0005)THEN
					sueldo = sueldo/30;
					--RAISE NOTICE 'codnomina: %', codnomina::INTEGER;
				END IF;

				--Calculamos el monto total
				montototal = sueldo*cantdias;
		
				--realizamos el calculo de la cantidad de cuotas pendientes
				IF(antiguedad>=1 AND antiguedad<=4) THEN

					cuotaspend = 5 - antiguedad;				

					WHILE (i<=antiguedad) LOOP
					
						IF (i=1) THEN
							monto = sueldo * cuo1;
						ELSE
						     IF (i=2) THEN
							monto := sueldo * cuo2;
						     END IF;

						     IF(i=3) THEN
							monto := sueldo * cuo3;
						     END IF;

						     IF (i=4) THEN
							monto := sueldo * cuo4;
						     END IF;
						     
						     IF (i=5) THEN
							monto := sueldo * cuo5;
						     END IF;
						END IF;

						descripcion = 'CUOTA NRO '||i;
						INSERT INTO sbp_det_pincentivo_vivienda(
							codincviv, codper, cuacan, pagemi, observacion)
						VALUES (codigo, codpersonal, i, monto, descripcion);
						i = i+1;
						respuesta  = TRUE;
					END LOOP;
				
					WHILE (j<=cuotaspend) LOOP
						--i almacena la informacion de cuantas cuotas se registraron
						
						IF (i=1) THEN
							monto = sueldo * cuo1;
						ELSE
						     IF (i=2) THEN
							monto := sueldo * cuo2;
						     END IF;

						     IF(i=3) THEN
							monto := sueldo * cuo3;
						     END IF;

						     IF (i=4) THEN
							monto := sueldo * cuo4;
						     END IF;
						     
						     IF (i=5) THEN
							monto := sueldo * cuo5;
						     END IF;
						END IF;

						descripcion = 'CUOTA NRO '||i;
						
						IF(bandera=TRUE) THEN

							SELECT proxcumpleanios(fechaing::TEXT) INTO fechaven;
							
							INSERT INTO sbp_det_pincentivo_vivienda(
            							codincviv, codper, cuacan, pagemi, observacion, fecvenpag)
								VALUES (codigo, codpersonal, i, monto, descripcion, fechaven::DATE);
							bandera = FALSE;
						ELSE
							anio = SUBSTRING(fechaven,0,5);
							fechaven  = SUBSTRING(fechaven,5,char_length(fechaven)); 

							fechaven = (anio::INTEGER+1)||fechaven;
							
							INSERT INTO sbp_det_pincentivo_vivienda(
            							codincviv, codper, cuacan, pagemi, observacion, fecvenpag)
								VALUES (codigo, codpersonal, i, monto, descripcion, fechaven::DATE);

						END IF;
						--RAISE NOTICE 'fechaven: %', fechaven;
						i = i+1;
						j = j+1;
						respuesta  = TRUE;
					END LOOP;

					
				ELSE
					cuotaspend = 0;
					INSERT INTO sbp_det_pincentivo_vivienda(
							codincviv, codper, cuacan, pagemi, observacion)
						VALUES (codigo, codpersonal, '1', montototal, 'PAGO UNICO');
					respuesta  = TRUE;
		
				END IF;

				--Actualizamos el registro de Plan Incentivo de Vivienda
				UPDATE sbp_pincentivo_vivienda
				   SET antper=antiguedad, montotapo=montototal, estben=estatus, 
				       feccom=feccomite::DATE, cuopen=cuotaspend, obscom=obs, codperi=periodo, anocur=anno
				 WHERE codincviv= codigo AND codper= codpersonal;
			END;

		END LOOP;
		CLOSE cursor1;
		
		RETURN respuesta; 
	END;
	$$;


