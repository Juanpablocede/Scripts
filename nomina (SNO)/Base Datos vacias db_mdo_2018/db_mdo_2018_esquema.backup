CREATE FUNCTION calcularantiguedad(fecha_ingreso text, fecha_hasta text) RETURNS numeric
    LANGUAGE plpgsql STABLE
    AS $$
DECLARE
  xantiguedad CHARACTER VARYING(40);
  xtxt_antiguedad CHARACTER VARYING(40);
BEGIN
  xtxt_antiguedad := age(fecha_hasta::timestamp,fecha_ingreso::timestamp);
  xantiguedad = substring(xtxt_antiguedad,0,3);
  RETURN xantiguedad;
END;
$$;


