-- Name: fsab_obtenerproxcorrelativo(character varying, character varying, numeric); Type: FUNCTION; Schema: public; Owner: postgres
--

CREATE FUNCTION fsab_obtenerproxcorrelativo(tablename character varying, fieldname character varying, incvalue numeric) RETURNS numeric
    LANGUAGE plpgsql
    AS $$
DECLARE
 rec RECORD;
 var_max NUMERIC;
BEGIN
  FOR rec IN EXECUTE 'Select CAST(max('|| FieldName ||'::numeric) AS NUMERIC) as maxid from ' || TableName LOOP
   var_max := rec.maxid::NUMERIC;
  END LOOP;
 
  IF var_max IS NULL THEN
   RETURN 1;
  ELSE
   RETURN var_max + IncValue;
  END IF;
 
  RETURN 1;
END;
$$;


ALTER FUNCTION public.fsab_obtenerproxcorrelativo(tablename character varying, fieldname character varying, incvalue numeric) OWNER TO postgres;

