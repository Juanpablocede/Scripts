CREATE FUNCTION spg_dtmp_cmp_01(OUT codemp character, OUT procede character, OUT comprobante character, OUT fecha date, OUT codestpro1 character, OUT codestpro2 character, OUT codestpro3 character, OUT codestpro4 character, OUT codestpro5 character, OUT estcla character, OUT spg_cuenta character, OUT procede_doc character, OUT documento character, OUT operacion character, OUT codfuefin character, OUT descripcion character, OUT monto integer, OUT orden integer) RETURNS SETOF record
    LANGUAGE plpgsql
    AS $_$

DECLARE
  mcodemp 	ALIAS FOR $1;
  mprocede	ALIAS FOR $2;
  mcomprobante	ALIAS FOR $3;
  mfecha	ALIAS FOR $4;
  mcodestpro1	ALIAS FOR $5;
  mcodestpro2	ALIAS FOR $6;
  mcodestpro3	ALIAS FOR $7;
  mcodestpro4	ALIAS FOR $8;
  mcodestpro5	ALIAS FOR $9;
  mestcla 	ALIAS FOR $10;
  mspg_cuenta	ALIAS FOR $11;
  mprocede_doc	ALIAS FOR $12;
  mdocumento	ALIAS FOR $13;
  moperacion	ALIAS FOR $14;
  mcodfuefin	ALIAS FOR $15;
  mdescripcion	ALIAS FOR $16;
  mmonto	ALIAS FOR $17;
  morden	ALIAS FOR $18;
  mcuenta	INTEGER;
  existen_datos BOOLEAN;
  
cu01 CURSOR FOR 

SELECT
	spg_dtmp_cmp_copia.codemp,
	spg_dtmp_cmp_copia.procede,
	spg_dtmp_cmp_copia.comprobante,
	spg_dtmp_cmp_copia.fecha,
	spg_dtmp_cmp_copia.codestpro1,
	spg_dtmp_cmp_copia.codestpro2,
	spg_dtmp_cmp_copia.codestpro3,
	spg_dtmp_cmp_copia.codestpro4,
	spg_dtmp_cmp_copia.codestpro5,
	spg_dtmp_cmp_copia.estcla,
	spg_dtmp_cmp_copia.spg_cuenta,
	spg_dtmp_cmp_copia.procede_doc,
	spg_dtmp_cmp_copia.documento,
	spg_dtmp_cmp_copia.operacion,
	spg_dtmp_cmp_copia.codfuefin,
	spg_dtmp_cmp_copia.descripcion,
	spg_dtmp_cmp_copia.monto,
	spg_dtmp_cmp_copia.orden
FROM
	spg_dtmp_cmp_copia
ORDER BY
	spg_dtmp_cmp_copia.codestpro5;
BEGIN
 existen_datos = FALSE;

OPEN cu01;
<<buscar_datos>>
 LOOP
   BEGIN
     FETCH cu01 INTO
	  mcodemp,
	  mprocede,
	  mcomprobante,
	  mfecha,
	  mcodestpro1,
	  mcodestpro2,
	  mcodestpro3,
	  mcodestpro4,
	  mcodestpro5,
	  mestcla,
	  mspg_cuenta,
	  mprocede_doc,
	  mdocumento,
	  moperacion,
	  mcodfuefin,
	  mdescripcion,
	  mmonto,
	  morden;
	  IF NOT FOUND THEN
             EXIT buscar_datos;
          END IF;
            
	  mcuenta:=0;
	  mcuenta:=COUNT(*) FROM spg_dtmp_cmp_temporal WHERE codemp=mcodemp AND procede=mprocede AND comprobante=mcomprobante AND fecha=mfecha AND codestpro1=mcodestpro1 AND codestpro2=mcodestpro2 AND codestpro3=mcodestpro3 AND codestpro4=mcodestpro4 AND codestpro5=mcodestpro5 AND estcla=mestcla AND spg_cuenta=mspg_cuenta AND procede_doc=mprocede_doc AND documento=mdocumento AND operacion=moperacion; 

	  IF mcuenta=0 THEN
		 INSERT INTO spg_dtmp_cmp_temporal VALUES(mcodemp,mprocede,mcomprobante,mfecha,mcodestpro1,mcodestpro2,mcodestpro3,mcodestpro4,mcodestpro5,mestcla,mspg_cuenta,mprocede_doc,mdocumento,moperacion,mcodfuefin,mdescripcion,mmonto,morden);
	  END IF;

     RETURN NEXT;
   END;
 END LOOP;
 CLOSE cu01;
END;
$_$;


