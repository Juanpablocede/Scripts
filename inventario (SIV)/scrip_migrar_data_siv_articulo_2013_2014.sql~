INSERT INTO siv_articulo
SELECT 
	siv_articulo.* 
FROM 
	dblink('dbname=db_mdo_2013', 
	'SELECT 
		codemp, 
		codart, 
		denart, 
		codtipart, 
		codunimed, 
		feccreart, 
		obsart, 
		exiart, 
		exiiniart, 
		minart, 
		maxart,
		reoart, 
		prearta, 
		preartb, 
		preartc, 
		preartd, 
		fecvenart,
		codcatsig, 
		spg_cuenta, 
		sc_cuenta, 
		pesart, 
		altart,
		ancart, 
		proart, 
		ultcosart, 
		cosproart, 
		fotart, 
		serart, 
		ubiart, 
		docart, 
		fabart, 
		codmil, 
		codact, 
		estact, 
		codseg,
		codfami, 
		codclase, 
		codprod, 
		estartgen, 
		codartpri, 
		lote, 
		carcom, 
		cod_pro, 
		sc_cuentainv  
	FROM siv_articulo
	WHERE
		codart::INTEGER BETWEEN 2490 AND 3603') AS 
	siv_articulo(
		codemp char(4), 
		codart char(20), 
		denart char(254), 
		codtipart char(4), 
		codunimed char(4), 
		feccreart date,
		obsart text, 
		exiart double precision , 
		exiiniart double precision, 
		minart double precision, 
		maxart double precision,
		reoart double precision, 
		prearta double precision, 
		preartb double precision, 
		preartc double precision, 
		preartd double precision, 
		fecvenart date,
		codcatsig varchar(15), 
		spg_cuenta varchar(25), 
		sc_cuenta varchar(25), 
		pesart double precision, 
		altart double precision, 
		ancart double precision, 
		proart double precision, 
		ultcosart double precision, 
		cosproart double precision, 
		fotart varchar(500), 
		serart varchar(25), 
		ubiart varchar(10), 
		docart varchar(20), 
		fabart varchar(100), 
		codmil varchar(15), 
		codact varchar(15), 
		estact varchar(1), 
		codseg varchar(2),
		codfami varchar(4), 
		codclase varchar(6), 
		codprod varchar(8), 
		estartgen varchar(1), 
		codartpri varchar(20), 
		lote varchar(10), 
		carcom varchar(1), 
		cod_pro varchar(10), 
		sc_cuentainv varchar(25));


